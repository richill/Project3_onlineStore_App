<h1>Listing productmains</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @productmains.each do |productmain| %>
  <tr>
    <td><%= productmain.name %></td>
    <td><%= link_to 'Show', productmain %></td>
    <td><%= link_to 'Edit', edit_productmain_path(productmain) %></td>
    <td><%= link_to 'Destroy', productmain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

