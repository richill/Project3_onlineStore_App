<h1>Listing product_brand_names</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_brand_names.each do |product_brand_name| %>
  <tr>
    <td><%= product_brand_name.name %></td>
    <td><%= link_to 'Show', product_brand_name %></td>
    <td><%= link_to 'Edit', edit_product_brand_name_path(product_brand_name) %></td>
    <td><%= link_to 'Destroy', product_brand_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

